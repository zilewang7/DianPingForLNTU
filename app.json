{
  "expo": {
    "name": "工大点评",
    "slug": "DianPingForLNTU",
    "version": "0.1.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "允许获取图片",
        "NSPhotoLibraryAddUsageDescription": "允许保存图片到本地"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.ddc777.DianPingForLNTU"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "允许获取图片"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "允许获取图片",
          "savePhotosPermission": "允许保存图片到本地",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7ed004de-f1c3-475b-8ad9-5f990e570bd9"
      }
    }
  }
}