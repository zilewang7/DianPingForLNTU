{
  "expo": {
    "name": "DianPingForLNTU",
    "slug": "DianPingForLNTU",
    "version": "0.1.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "允许获取图片"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "允许获取图片",
          "savePhotosPermission": "允许保存图片到本地",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}